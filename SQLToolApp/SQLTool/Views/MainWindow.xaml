<dx:ThemedWindow xmlns:Views="clr-namespace:SQLTool.Views"  x:Class="SQLTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        Title="SQL Tool" Height="500" Width="400">
    <Grid RenderTransformOrigin="0.489,0.508">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Views:SqlControlView x:Name="ctrlTo" Grid.Column="0" Margin="2,0,2,0" />
            <Views:SqlControlView x:Name="ctrlFrom" Grid.Column="1" Margin="2,0,2,0" />
        </Grid>
    </Grid>
</dx:ThemedWindow>
